{
  "connection": "default",
  "collectionName": "",
  "info": {
    "name": "notification",
    "description": "Notificações ( PUSH )"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "title": {
      "configurable": true,
      "type": "string"
    },
    "subtitle": {
      "configurable": true,
      "type": "string"
    },
    "body": {
      "configurable": true,
      "type": "text"
    },
    "priority": {
      "configurable": true,
      "required": true,
      "enum": [
        "default",
        "normal",
        "high"
      ],
      "default": "normal",
      "type": "enumeration"
    },
    "sendDate": {
      "configurable": true,
      "required": true,
      "type": "date"
    },
    "pending": {
      "configurable": true,
      "type": "boolean",
      "default": true
    },
    "users": {
      "collection": "user",
      "dominant": true,
      "via": "notifications",
      "plugin": "users-permissions"
    },
    "data": {
      "type": "json",
      "configurable": true
    }
  }
}